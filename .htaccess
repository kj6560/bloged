Options +FollowSymLinks
RewriteEngine On
# Redirect requests to index.php
RewriteCond %{DOCUMENT_ROOT}%{REQUEST_URI} !=/index.php
RewriteCond %{DOCUMENT_ROOT}%{REQUEST_URI} !.*\.png$ [NC]
RewriteCond %{DOCUMENT_ROOT}%{REQUEST_URI} !.*\.jpg$ [NC]
RewriteCond %{DOCUMENT_ROOT}%{REQUEST_URI} !.*\.css$ [NC]
RewriteCond %{DOCUMENT_ROOT}%{REQUEST_URI} !.*\.gif$ [NC]
RewriteCond %{DOCUMENT_ROOT}%{REQUEST_URI} !.*\.js$ [NC]
RewriteCond %{DOCUMENT_ROOT}%{REQUEST_URI}\.php -f
RewriteRule .* /index.php 